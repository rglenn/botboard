                      00002 Round   macro aa,bb
                      00003         LOCAL rr = aa/bb
                      00004         LOCAL d1 = aa - rr*bb
                      00005         LOCAL d2 = (rr+1)*bb - aa
                      00006 RoundResult = rr
                      00007         if d1 >= d2
                      00008 RoundResult++
                      00009         endif
                      00010         endm
                      00011 
  00000000            00012 AbsResult SET 0
                      00013 Abs             macro nr
